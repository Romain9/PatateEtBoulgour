{{>components/header}}



<div class="entetePr">
    <h1 class="name">Profil de {{user.firstName}}, {{user.lastName}}</h1>
    <p class="age">{{user.age}}</p>
    <p class="gender">{{user.gender}}</p>
    <p class="adresse">{{user.address}}</p>
    <ul class="pathology">
        {{#user.pathologies}}
            <li>{{label}}</li>
        {{/user.pathologies}}
    </ul>
</div>

